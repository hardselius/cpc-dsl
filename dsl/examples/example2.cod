import float

Component add      '''adds'''
  in  ( Float a
      , Float b )
  out ( Float q )
  Atom <SHIT>
  {}

Component mul      '''multiplies'''
  in  ( Float a
      , Float b )
  out ( Float q )
  Atom <SHIT>
  {}

Component addmul      '''Test function that adds and multiplies'''
  in  ( Float a
      , Float b
      , Float c )
  out ( Float q )
  Network
  { cAdd = add in.a, in.b
    cMul = mul cAdd.out.q, in.c
    out.q <- cMul.out.q }
