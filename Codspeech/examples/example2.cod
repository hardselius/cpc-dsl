import float

type test '''test crap'''
  (int : a)

atom python add      '''Add two floating point numbers: q=a+b'''
  in  ( float a
      , float b )
  out ( float q    '''a+b'''
      )
  otions ( "function" : "builtin.float.add"
         , "import "  : "buitlin.float" )

atom python mul      '''Multiply two floating point numbers: q=a*b'''
  in  ( Float a
      , Float b )
  out ( Float q    '''a*b'''
      )
  options ( "function" : "builtin.float.add"
          , "import "  : "buitlin.float" )

network addmul      '''Test function that adds and multiplies'''
  in  ( Float a       '''First input'''
      , Float b       '''Second input'''
      , Float c       '''Third input'''
      )
  out ( Float q       '''q = (a+b)*c'''
      )
  {
    cMul = mul ((add (in.a, in.b)).out.q, in.c)
    out.q <- cMul.out.q
  }