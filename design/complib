import funclib

Component add           ''uses function fAdd: q = a+b''
  in  Float a default 0 ''input a has default value 0''
     ,Float b           ''input b''
  out Float q           ''output q''
  q = fAdd a b          ''q = a+b''


Coomponent mul
  in  Float a
     ,Float b
  out Float q
  q = fMul a b


Component apply
  in  Fun  f
     ,List l
  out f.type q
  q = f l