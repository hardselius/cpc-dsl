import funclib

Component add           ''uses function fAdd: q = a+b''
  in  Float a default 0 ''input a has default value 0''
     ,Float b           ''input b''
  out Float q           ''output q''
  q = fAdd a b          ''q = a+b''


Component mul
  in  Float a
     ,Float b
  out Float q
  c1 := fMul a b
  q1 <- c1.out.q1
  q2 <- c1.out.q2
  # or
  out.q1 out.q2 <- fMul in.a in.b


Component apply
  in  Fun  f
     ,List l
  out f.type q
  q = f l