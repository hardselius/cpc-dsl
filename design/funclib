Function fAdd         ''add function defenition''
  in  Float a         ''input a''
     ,Float b         ''input b''
  out Float q         ''output q''
  external float::add ''external pyhton function float::add''
           in.a = a   ''connect float::add.in.a to a''
           in.b = b   ''connect float::add.in.b to b''

Function fMul                          ''add function defenition''
  extfun float::mul in.a, in.b, out.q  ''external pyhton function float::add''
  in  Float a = extfun.in.a                   ''connect input a to in.a''
     ,Float b = in.b                   ''connect input b to in.b''
  out Float q = out.q                  ''connect output q to q''